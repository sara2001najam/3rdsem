int[] array = new int[20];

Console.WriteLine("Enter values for arrays: "); 
for(int i =0; i < 20; i++)
{
    Console.Write("element {0}: ", i);
        array[i] = Convert.ToInt32(Console.ReadLine());
    
}
Console.WriteLine();
Console.Write("Multiplying elements by 5: "); 
for(int i =0; i < array.Length; i++)
{
   Console.Write("{0}\t", array[i] * 5);    
    
}
